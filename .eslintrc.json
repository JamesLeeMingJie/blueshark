{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard",
    "next/core-web-vitals"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [
      2,
      "always"
    ],
    "@typescript-eslint/ban-ts-comment": 0,
    "spaced-comment": 0,
    "camelcase": 0,
    "space-before-function-paren": 0,
    "comma-dangle": 0,
    "multiline-ternary": 0,
    "dot-notation": 0,
    "react-hooks/exhaustive-deps": 0,
    "no-useless-escape": 0,
    "indent": 0,
    "react/display-name": 0,
    "no-unused-vars": 1,
    "@next/next/no-img-element": 0,
    "quotes": [0, "single"],
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  }
}